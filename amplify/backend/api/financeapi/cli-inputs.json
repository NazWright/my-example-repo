{
  "version": 1,
  "paths": {
    "/plaid": {
      "name": "/plaid",
      "lambdaFunction": "createPlaidAuthToken",
      "permissions": {
        "setting": "open"
      }
    },
    "/plaid/access-token": {
      "name": "/plaid/access-token",
      "lambdaFunction": "fetchPlaidAccessToken",
      "permissions": {
        "setting": "open"
      }
    },
    "/plaid/accounts": {
      "name": "/plaid/accounts",
      "lambdaFunction": "fetchPlaidAccountInformation",
      "permissions": {
        "setting": "open"
      }
    }
  }
}